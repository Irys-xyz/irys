{
  "title": "Packing & Storage",
  "uid": "irys-packing-storage-detailed",
  "tags": [
    "irys",
    "packing",
    "storage",
    "mining"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 2,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "node",
        "label": "Node",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "query": "label_values(irys_storage_modules_total, job)",
        "refresh": 2,
        "includeAll": true,
        "allValue": ".*",
        "multi": false,
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "options": [],
        "sort": 1,
        "regex": ""
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Storage Overview",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 1
      },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "Storage Modules",
      "description": "Total storage modules configured",
      "gridPos": {
        "x": 0,
        "y": 1,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "sum(irys_storage_modules_total{job=~\"$node\"})",
          "legendFormat": "modules",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "blue",
                "value": null
              }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Active Packing Workers",
      "description": "Currently active packing workers",
      "gridPos": {
        "x": 6,
        "y": 1,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "sum(irys_packing_active_workers{job=~\"$node\"})",
          "legendFormat": "workers",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "blue",
                "value": null
              }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Cached Chunks",
      "description": "Number of chunks in cache",
      "gridPos": {
        "x": 12,
        "y": 1,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "sum(irys_cache_chunk_count{job=~\"$node\"})",
          "legendFormat": "chunks",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "blue",
                "value": null
              }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Mining Solutions",
      "description": "Total mining solutions found",
      "gridPos": {
        "x": 18,
        "y": 1,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "sum(irys_mining_solutions_found_total{job=~\"$node\"})",
          "legendFormat": "solutions",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        }
      }
    },
    {
      "type": "row",
      "title": "Packing Activity",
      "gridPos": {
        "x": 0,
        "y": 5,
        "w": 24,
        "h": 1
      },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Active Packing Workers Over Time",
      "description": "Number of active packing workers",
      "gridPos": {
        "x": 0,
        "y": 6,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "irys_packing_active_workers{job=~\"$node\"}",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Packing Semaphore Available",
      "description": "Available packing permits over time",
      "gridPos": {
        "x": 12,
        "y": 6,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "irys_packing_semaphore_available{job=~\"$node\"}",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2
          }
        }
      }
    },
    {
      "type": "row",
      "title": "Cache",
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 24,
        "h": 1
      },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Cache Chunk Count Over Time",
      "description": "Number of cached chunks",
      "gridPos": {
        "x": 0,
        "y": 13,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "irys_cache_chunk_count{job=~\"$node\"}",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Cache Chunk Size",
      "description": "Size of cached chunks in bytes",
      "gridPos": {
        "x": 12,
        "y": 13,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "irys_cache_chunk_size_bytes{job=~\"$node\"}",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "decbytes",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2
          }
        }
      }
    },
    {
      "type": "row",
      "title": "Mining & Commitments",
      "gridPos": {
        "x": 0,
        "y": 19,
        "w": 24,
        "h": 1
      },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Mining Solutions Found",
      "description": "Rate of mining solutions discovered",
      "gridPos": {
        "x": 0,
        "y": 20,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "rate(irys_mining_solutions_found_total{job=~\"$node\"}[$__rate_interval])",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Pledge Txs Posted",
      "description": "Rate of pledge transactions posted",
      "gridPos": {
        "x": 12,
        "y": 20,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "rate(irys_commitment_pledge_tx_posted_total{job=~\"$node\"}[$__rate_interval])",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Storage Modules by Node",
      "description": "Number of storage modules per node over time",
      "gridPos": {
        "x": 0,
        "y": 26,
        "w": 24,
        "h": 6
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "irys_storage_modules_total{job=~\"$node\"}",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          }
        }
      }
    },
    {
      "type": "row",
      "title": "Data Sync Performance",
      "gridPos": { "x": 0, "y": 32, "w": 24, "h": 1 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Data Sync Tick Duration (p50, p95, p99)",
      "description": "Data sync service tick cycle duration",
      "gridPos": { "x": 0, "y": 33, "w": 24, "h": 8 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.50, sum by (le, service_name) (rate(traces_span_metrics_duration_milliseconds_bucket{job=~\"$node\", span_name=\"dispatch_chunk_requests\"}[$__rate_interval])))",
          "legendFormat": "{{service_name}} p50",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.95, sum by (le, service_name) (rate(traces_span_metrics_duration_milliseconds_bucket{job=~\"$node\", span_name=\"dispatch_chunk_requests\"}[$__rate_interval])))",
          "legendFormat": "{{service_name}} p95",
          "refId": "B"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.99, sum by (le, service_name) (rate(traces_span_metrics_duration_milliseconds_bucket{job=~\"$node\", span_name=\"dispatch_chunk_requests\"}[$__rate_interval])))",
          "legendFormat": "{{service_name}} p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 }
        }
      }
    }
  ]
}
